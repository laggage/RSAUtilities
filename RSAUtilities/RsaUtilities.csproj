<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>RSAUtilities</AssemblyName>
    <PackageVersion>0.0.1</PackageVersion>
    <Authors>Laggage</Authors>
    <PackageId>RSAUtilities</PackageId>
    <Title>RSA相关辅助方法</Title>
    <Description>提供一些扩展方法, 使得可以方便的从密钥文件中读取RSA私钥</Description>
    <PackageIcon>icon.png</PackageIcon>

    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
   
  </PropertyGroup>

  <ItemGroup>
    <Content Include="images/icon.png">
      <Pack>true</Pack>
      <PackagePath></PackagePath>
    </Content>
  </ItemGroup>

</Project>
